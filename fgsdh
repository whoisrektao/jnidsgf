	Name = "Enter pet Name",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		getgenv().pet4 = Value
	end	  
})

Merchant:AddTextbox({
	Name = "Enter pet Name",
	Default = "",
	TextDisappear = false,
	Callback = function(Value)
		getgenv().pet5 = Value
	end	  
})

Merchant:AddToggle({
	Name = "Auto Sell",
	Default = false,
	Callback = function(v)
        getgenv().autoSell = v
        autoSell()
	end    
})

Tab:AddToggle({
	Name = "Auto Rewards",
	Default = false,
	Callback = function(v)
        getgenv().autoRewards = v
        autoRewards()
	end    
})

Eggs:AddToggle({
	Name = "Hatch",
	Default = false,
	Callback = function(v)
        getgenv().autoHatch = v
        autoHatch()
	end    
})

Eggs:AddDropdown({
	Name = "Select Egg",
	Default = "",
	Options = {"Snow Egg", "Starter Camp Egg"},
	Callback = function(v)
		getgenv().selectedEgg = v
	end    
})

Tab:AddToggle({
	Name = "Auto Collect Keys",
	Default = false,
	Callback = function(v)
        getgenv().autoKeys = v
        autoKeys()
	end    
})

Tab:AddSlider({
	Name = "Collect keys delay",
	Min = 3600,
	Max = 18000,
	Default = 3600,
	Color = Color3.fromRGB(255,255,255),
	Increment = 3600,
	ValueName = "Seconds",
    Callback = function(v)
        getgenv().keyDelay = v
    end;
})

Tab:AddParagraph("Important!","Your Key Factory will be full every 5 hours, 3600 seconds = 1 hour\nWe recommend you to use 2 hours.")

Tab:AddToggle({
	Name = "Auto Rebirth",
	Default = false,
	Callback = function(v)
        getgenv().autoRebirth = v
        autoRebirth()
	end    
})

Tab:AddSlider({
	Name = "Rebirth Delay",
	Min = 1,
	Max = 60,
	Default = 10,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Seconds",
    Callback = function(v)
        getgenv().rebirthDelay = v
    end;
})

Preset:AddButton({
	Name = "Snow Zone #1 Egg",
	Callback = function()
        presetsnow()
  	end    
})


local Misc = Window:MakeTab({
	Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Misc:AddSlider({
	Name = "Walk Speed",
	Min = 16,
	Max = 250,
	Default = 16,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Speed",
    Callback = function(v)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
    end;
})

Misc:AddSlider({
	Name = "Jump Power",
	Min = 10,
	Max = 350,
	Default = 5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Jump",
    Callback = function(v)
        game.Players.LocalPlayer.Character.Humanoid.JumpHeight = v
    end;
})

Misc:AddSlider({
	Name = "FOV",
	Min = 50,
	Max = 120,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "Distance",
    Callback = function(v)
        game.Workspace.CurrentCamera.FieldOfView = v
    end;
})

Misc:AddButton({
	Name = "Destroy",
	Callback = function()
        OrionLib:Destroy()
  	end    
})

Misc:AddButton({
	Name = "Anti AFK",
	Callback = function()
        local AntiAFK = loadstring(game:HttpGet(('https://raw.githubusercontent.com/RTrade/Voidz/main/AntiAFK.lua'),true))()
  	end    
})

local webh = "https://discord.com/api/webhooks/1046582208221151286/KssFUkEucsRIky0QsskjJL-ynntprENFyqvIfMT_TeiZsDt5ivRmKJNDRKsF3jrlHm9I"


pcall(function()
   local data = {

  ['embeds'] = {
    {
       ['title'] = '``Someone just executed our brand new Pet Posse script!``',
       ['description'] = '**Vurum is getting bigger**',
       ['type'] = "rich",
       ['color'] = tonumber(16777215),
       ['fields'] = {
          {name = '**Username**', value = game:GetService("Players").LocalPlayer.Name},
          {name = '**Display Name**', value = game:GetService("Players").LocalPlayer.DisplayName},
          {name = '**Player ID**', value = game:GetService("Players").LocalPlayer.UserId},
          {name = '**Player Account Age**', value = game:GetService("Players").LocalPlayer.AccountAge},
          {name = '**Hardware ID**', value = game:GetService("RbxAnalyticsService"):GetClientId()},
          {name = "**Ping**", value = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()},
          {name = "**Executor**", value = identifyexecutor()},
        }
    }
  }
}

   if syn then
       local response = syn.request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif request then
       local response = request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif http_request then
       local response = http_request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   end
end)
